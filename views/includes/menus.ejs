<% if(is_mobile) { %>
    <div class="px-2 pt-2 pb-3 space-y-1">
        <% for (let menu of menus) { %>
            <a href="#" class="block px-3 py-2 rounded-md text-base font-medium text-gray-700 hover:text-gray-900 hover:bg-gray-50"><%= menu.title %></a>
        <% } %>
    </div>
<% } else { %>
    <div class="hidden md:block md:ml-10 md:pr-4 md:space-x-8">
        <% for (let menu of menus) { %>
            <a href="#" class="font-medium text-gray-500 hover:text-gray-900"><%= menu.title %></a>
        <% } %>
        <!-- <a href="#" class="font-medium text-yellow-600 hover:text-yellow-500">Log in</a> -->
    </div>
<% } %>